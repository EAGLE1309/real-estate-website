{
  "env": {
    "node": true
  },
  "extends": [
    "react-app",
    "eslint:recommended"
  ],
  "plugins": ["import"],
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".ts", ".tsx"],
      }
    }
  },
  "rules": {
    "no-sparse-arrays": "warn",
    "no-unused-vars": "error",
    "complexity": "warn",
    "no-console": "warn"
  }
}